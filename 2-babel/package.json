{
  "name": "npm-es-modules-2-babel",
  "version": "0.0.2",
  "description": "Breakdown of 7 different ways to use ES modules with NPM today.",
  "main": "dist/main.js",
  "module": "module.js",
  "browser": "dist/browser.js",
  "repository": "transitive-bullshit/npm-es-modules",
  "author": "Travis Fischer <travis@automagical.ai>",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean && babel *.mjs lib/*.mjs test/*.mjs --out-dir dist",
    "clean": "del dist",
    "prepublishOnly": "yarn run build",
    "pretest": "yarn run build",
    "test": "ava -v dist/test && standard *.mjs lib test"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "npm",
    "es",
    "esm",
    "modules",
    "commonjs"
  ],
  "devDependencies": {
    "ava": "^1.0.0-beta.5.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "del-cli": "^1.1.0",
    "standard": "^11.0.0"
  },
  "dependencies": {
    "get-pixels": "^3.3.0",
    "ow": "^0.4.0"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4"
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
